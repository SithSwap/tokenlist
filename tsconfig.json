{
	"compilerOptions": {
		"allowJs": true,
		"checkJs": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"resolveJsonModule": true,
		"skipLibCheck": true,
		"sourceMap": true,
		"strict": true,
		"noImplicitAny": false,
		"target": "es2020",
		"module": "commonjs",
		"declaration": true,
	},
	"include": ["./test", "./src",]
}
